        {
            "type": "java",
            "name": "CodeLens (Launch) - BigFileReader",
            "request": "launch",
            "mainClass": "com.mee.manage.util.BigFileReader",
            "projectName": "erp-manage"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - MD5Util",
            "request": "launch",
            "mainClass": "com.mee.manage.util.MD5Util",
            "projectName": "erp-manage"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "MeeStoreManageApplication<erp-manage>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.mee.ManageApplication",
            "projectName": "erp-manage",
            "args": ""
